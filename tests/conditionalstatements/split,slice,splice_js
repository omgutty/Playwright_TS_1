let n2 = [10,20,30,40,50,60];
let n3 =n2.splice(2,0,100);
console.log(n2);
console.log(n3);


//replace 
let n4= [10,20,30,40,50,60];
let n5=n4.splice(2,1,0);
console.log(n4);
console.log(n5);



//swipe 

let n6= [10,20,30,40,50];

let n7=n6.splice(3,2,50,40);

console.log(n6);
console.log(n7);




